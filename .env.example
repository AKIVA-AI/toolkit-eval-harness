# AKIVA Eval Harness Configuration

# General Settings
EVAL_HARNESS_HOME=/app
LOG_LEVEL=INFO
VERBOSE=false

# Suite Pack Settings
ENABLE_SIGNING=false
SIGNING_KEY_PATH=
VERIFY_SIGNATURES=true

# Evaluation Settings
DEFAULT_SCORER=exact_match
PARALLEL_WORKERS=4
BATCH_SIZE=100

# Regression Settings
REGRESSION_THRESHOLD=0.05
FAIL_ON_REGRESSION=true

# Output Settings
OUTPUT_FORMAT=json
SAVE_PREDICTIONS=true
SAVE_DETAILED_RESULTS=true

# Performance
MAX_MEMORY_MB=4096
TIMEOUT_SECONDS=300

